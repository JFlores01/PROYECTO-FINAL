<%- include("templates/cabeceralogin.ejs", {tituloWeb:'Login'}) %>






  <section class="resultados" id="resultados mt-5">
    <div class="container py-3 mb-3">
      <div class="text-center">
        <p>&nbsp;</p>
        <br>
        <h2>REGISTRO</h2>
        <p class="lead">Regístrate para acceder</p>
      </div>
      <div class="container mt-3">
        <form class="formulario__register" id="formulario__register">
          <div class="mb-3 mt-3">
            <label for="nombre"> <p>Nombre:</p></label>
            <input type="nombre" class="form-control" id="nombre" placeholder="Introduzca nombre" name="nombre">
          </div>
          <div class="mb-3">
            <label for="pwd"><p>Correo Electrónico:</p></label>
            <input type="email" class="form-control" id="email" placeholder="Introduzca email" name="email">
          </div>
          <div class="mb-3">
            <label for="pwd"><p>Contraseña:</p></label>
            <input type="password" class="form-control" id="pswd" placeholder="Introduzca contraseña" name="pswd">
          </div>

          <div class="mb-3">
            <label for="pwd"><p>Repetir Contraseña:</p></label>
            <input type="password" class="form-control" id="pswd2" placeholder="Introduzca contraseña" name="pswd2">
          </div>

          <div class="mb-3">
            <label for="pwd"><p>Repetir Contraseña:</p></label>
            <input type="password" class="form-control" id="pwd" placeholder="Introduzca contraseña" name="pswd">
          </div>
          <button type="submit" class="btn btn-primary">Submit</button> <br><br>
          <a href="/">Iniciar Sesión</a>
        </form>
      </div>
    </div>
  </section>

  <script src="/js/controlRegistro.js"></script>
  <%- include("templates/footer") %>